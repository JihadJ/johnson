<!DOCTYPE html>
<html>
  <head>
    <script>
      var numKids;
      var job;
      var nameSpouse;
      var location;
      function tellFortune(job, location, nameSpouse, numKids) {
        alert("You will be a " + job + " in " + location + ", and married to " + nameSpouse + " with " + numKids + " kids.");
  }
      tellFortune("Programmer", "Austin", "Liz", 2);
      tellFortune("Game designer", "Japan", "Carolyn", 5);
      tellFortune("Poet", "Sweden", Riveria", 3);
    </script>
  </head>
  <body>
    <h1>Fortune Teller</h1>
  </body>
</html>
